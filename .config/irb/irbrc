
# Make colors look somewhat like VS Code
Reline::Face.config(:completion_dialog) do |conf|
  conf.define :default, foreground: "#d4d4d4", background: "#1e1e1e"
  conf.define :enhanced, foreground: "#d4d4d4", background: "#37373d"
  conf.define :scrollbar, foreground: "#424242", background: "#2d2d2d"
end
